{
  "name": "vibe-music",
  "version": "0.0.1",
  "description": "vibe-music",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuenoleK/vibe.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "start:api": "better-npm-run start-api",
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "build:api": "gulp",
    "db:init": "better-npm-run db-init",
    "db:insert": "better-npm-run db-insert",
    "db-init:prod": "better-npm-run db-init-prod"
  },
  "betterScripts": {
    "db-init": {
      "command": "cd build && node ./api/database/init.js",
      "env": {
        "DB_ENV": "dev"
      }
    },
    "db-insert": {
      "command": "cd build && node ./api/database/insertions.js",
      "env": {
        "DB_ENV": "dev"
      }
    },
    "db-init-prod": {
      "command": "cd build && node ./api/database/init.js",
      "env": {
        "DB_ENV": "prod"
      }
    },
    "start-api": {
      "command": "cd build && node ./api/index.js",
      "env": {
        "DB_ENV": "prod"
      }
    }
  },
  "private": true,
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.7.2",
    "merge-stream": "^1.0.0",
    "mysql": "^2.11.1",
    "node-sass": "^3.10.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "sass-loader": "^4.0.2",
    "sqlite3": "^3.1.6",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "material-design-lite": "^1.2.1",
    "material-ui": "^0.16.4",
    "mysql2": "^1.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.0.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "sequelize": "^3.24.3"
  }
}
